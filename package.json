{
  "name": "stackerftp",
  "displayName": "StackerFTP - Professional SFTP/FTP Client",
  "description": "Professional SFTP, FTP & FTPS client for VS Code, Cursor, and Windsurf. Features a native Remote Explorer, bi-directional sync, upload on save, and advanced WebMaster tools.",
  "version": "1.1.1",
  "publisher": "yasinkuyu",
  "author": "Insya, Yasin Kuyu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yasinkuyu/StackerFTP.git"
  },
  "homepage": "https://github.com/yasinkuyu/StackerFTP",
  "bugs": {
    "url": "https://github.com/yasinkuyu/StackerFTP/issues"
  },
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "SCM Providers"
  ],
  "keywords": [
    "ftp",
    "sftp",
    "ftps",
    "ssh",
    "remote",
    "sync",
    "upload",
    "download",
    "deploy",
    "deployment",
    "webmaster",
    "file manager",
    "security",
    "cursor",
    "antigravity",
    "windsurf",
    "vscodium",
    "trae",
    "pearai"
  ],
  "activationEvents": [
    "onFileSystem:stackerftp-remote",
    "onSecondarySideBarView:stackerftp.remoteExplorerTree",
    "workspaceContains:**/.vscode/sftp.json",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "StackerFTP",
      "properties": {
        "stackerftp.showHiddenFiles": {
          "type": "boolean",
          "default": false,
          "description": "Show hidden files in remote explorer"
        },
        "stackerftp.confirmDelete": {
          "type": "boolean",
          "default": true,
          "description": "Confirm before deleting remote files"
        },
        "stackerftp.confirmSync": {
          "type": "boolean",
          "default": true,
          "description": "Confirm before syncing directories"
        },
        "stackerftp.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Auto refresh remote explorer after operations"
        },
        "stackerftp.transferConcurrency": {
          "type": "number",
          "default": 5,
          "minimum": 1,
          "maximum": 20,
          "description": "Number of concurrent file transfers"
        },
        "stackerftp.showWebMasterTools": {
          "type": "boolean",
          "default": true,
          "description": "Show web master tools in explorer"
        },
        "stackerftp.enableFileWatcher": {
          "type": "boolean",
          "default": false,
          "description": "Watch local files for changes"
        },
        "stackerftp.autoReconnect": {
          "type": "boolean",
          "default": true,
          "description": "Automatically reconnect when a connection drops"
        },
        "stackerftp.defaultSyntaxHighlighting": {
          "type": "boolean",
          "default": true,
          "description": "Enable syntax highlighting for remote files"
        },
        "stackerftp.useNativeTreeView": {
          "type": "boolean",
          "default": true,
          "description": "Use native TreeView with File Icon Theme support (requires reload). Disable to use WebView with preview support."
        },
        "stackerftp.downloadWhenOpenInRemoteExplorer": {
          "type": "boolean",
          "default": false,
          "description": "Download file instead of view content when opening in Remote Explorer"
        },
        "stackerftp.remoteExplorerSortOrder": {
          "type": "string",
          "default": "name",
          "enum": [
            "name",
            "size",
            "date",
            "type"
          ],
          "enumDescriptions": [
            "Sort by file name",
            "Sort by file size",
            "Sort by modification date",
            "Sort by file type (folders first)"
          ],
          "description": "Default sort order for Remote Explorer"
        },
        "stackerftp.remotes": {
          "type": "object",
          "default": {},
          "description": "Remote-FS Integration: Define reusable remote server configurations. Reference them in sftp.json with \"remote\": \"name\"",
          "additionalProperties": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "Display name for the remote"
              },
              "host": {
                "type": "string",
                "description": "Hostname or IP address"
              },
              "port": {
                "type": "number",
                "description": "Port number"
              },
              "protocol": {
                "type": "string",
                "enum": [
                  "sftp",
                  "ftp",
                  "ftps"
                ],
                "description": "Connection protocol"
              },
              "username": {
                "type": "string",
                "description": "Username for authentication"
              },
              "password": {
                "type": "string",
                "description": "Password for authentication"
              },
              "privateKeyPath": {
                "type": "string",
                "description": "Path to SSH private key"
              },
              "remotePath": {
                "type": "string",
                "description": "Default remote path"
              }
            },
            "required": [
              "host"
            ]
          }
        }
      }
    },
    "submenus": [
      {
        "id": "stackerftp.submenu",
