      "scm/title": [
        {
          "command": "stackerftp.uploadChangedFiles",
          "group": "stackerftp",
          "when": "scmProvider == git"
        }
      ],
      "scm/resourceGroup/context": [
        {
          "command": "stackerftp.uploadChangedFiles",
          "group": "inline",
          "when": "scmProvider == git && scmResourceGroup == workingTree"
        },
        {
          "command": "stackerftp.uploadChangedFiles",
          "group": "stackerftp",
          "when": "scmProvider == git && scmResourceGroup == workingTree"
        },
        {
          "command": "stackerftp.uploadChangedFiles",
          "group": "inline",
          "when": "scmProvider == git && scmResourceGroup == index"
        },
        {
          "command": "stackerftp.uploadChangedFiles",
          "group": "stackerftp",
          "when": "scmProvider == git && scmResourceGroup == index"
        }
      ],
      "editor/title": [
        {
          "command": "stackerftp.editInLocal",
          "when": "resourceScheme == stackerftp-remote",
          "group": "navigation@1"
        }
      ]
    },
    "views": {
      "stackerftp-explorer": [
        {
          "type": "webview",
          "id": "stackerftp.connectionForm",
          "name": "Connections",
          "when": "workspaceFolderCount > 0",
          "contextualTitle": "StackerFTP",
          "visibility": "visible",
          "icon": "$(plug)"
        },
        {
          "id": "stackerftp.remoteExplorerTree",
          "name": "Remote Explorer",
          "when": "workspaceFolderCount > 0 && !stackerftp.formVisible",
          "contextualTitle": "StackerFTP",
          "visibility": "visible",
          "icon": "$(folder-opened)"
        },
        {
          "id": "stackerftp.transferQueue",
          "name": "Transfer Queue",
          "when": "workspaceFolderCount > 0 && !stackerftp.formVisible",
          "contextualTitle": "StackerFTP",
          "visibility": "collapsed",
          "icon": "$(cloud-upload)"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "stackerftp-explorer",
          "title": "StackerFTP",
          "icon": "resources/activity-bar-icon.svg"
        }
      ]
    },
    "keybindings": [
      {
        "command": "stackerftp.uploadCurrentFile",
        "key": "ctrl+shift+u",
        "mac": "cmd+shift+u",
        "when": "editorTextFocus"
      },
      {
        "command": "stackerftp.download",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d"
      },
      {
        "command": "stackerftp.syncToRemote",
        "key": "ctrl+shift+alt+u",
        "mac": "cmd+shift+alt+u"
      },
      {
        "command": "stackerftp.syncToLocal",
        "key": "ctrl+shift+alt+d",
        "mac": "cmd+shift+alt+d"
      },
      {
        "command": "stackerftp.uploadChangedFiles",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u",
        "when": "workspaceFolderCount > 0"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": ".vscode/sftp.json",
        "url": "./schema/sftp.schema.json"
      }
    ]
  },
